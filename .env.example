# =============================================================================
# CONFIGURACIÓN DE LA APLICACIÓN YOLO ANNOTATOR CON JWT
# =============================================================================

# Base de datos
DATABASE_URL=sqlite:///./yolo_annotator.db

# JWT Security
JWT_SECRET_KEY=your-super-secret-jwt-key-change-this-in-production
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=1440

# Configuración del servidor
HOST=127.0.0.1
PORT=8002
DEBUG=True

# Configuración de archivos
MAX_FILE_SIZE_MB=50
ALLOWED_IMAGE_EXTENSIONS=jpg,jpeg,png,webp,gif,bmp

# Configuración de sesiones
MAX_SESSIONS_PER_USER=100
DEFAULT_CANVAS_SIZE=640

# Configuración de augmentación
AUGMENTATION_VARIANTS=negativo,brillo,espejo,rotacion,desenfoque,contraste
MAX_AUGMENTATION_WORKERS=4
